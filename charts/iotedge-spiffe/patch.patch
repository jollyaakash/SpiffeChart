From: Aakash Jolly <aajolly@microsoft.com>
Date: Thu, 14 Apr 2022 15:38:43 -0700
Subject: [PATCH] Use Lee debug build images with openssl, protobuf fixed

---
 charts/iotedge-spiffe/templates/agent-daemonset.yaml   | 2 +-
 charts/iotedge-spiffe/templates/server-deployment.yaml | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/charts/iotedge-spiffe/templates/agent-daemonset.yaml b/charts/iotedge-spiffe/templates/agent-daemonset.yaml
index 8d86636..e068ecc 100755
--- a/charts/iotedge-spiffe/templates/agent-daemonset.yaml
+++ b/charts/iotedge-spiffe/templates/agent-daemonset.yaml
@@ -19,7 +19,7 @@ spec:
       serviceAccountName: iotedge-spiffe-agent
       containers:
         - name: iotedge-spiffe-agent
-          image:  dmqttdemo.azurecr.io/identity/agent:latest
+          image:  lefitchereg1.azurecr.io/identity/agent:debug_build
           securityContext:
             privileged: true
             allowPrivilegeEscalation: true
diff --git a/charts/iotedge-spiffe/templates/server-deployment.yaml b/charts/iotedge-spiffe/templates/server-deployment.yaml
index 142b34b..56971dc 100755
--- a/charts/iotedge-spiffe/templates/server-deployment.yaml
+++ b/charts/iotedge-spiffe/templates/server-deployment.yaml
@@ -16,7 +16,7 @@ spec:
       serviceAccountName: iotedge-spiffe-server
       containers:
         - name: iotedge-spiffe-server
-          image: dmqttdemo.azurecr.io/identity/server:latest
+          image: lefitchereg1.azurecr.io/identity/server:debug_build
           securityContext:
             privileged: true
             allowPrivilegeEscalation: true
